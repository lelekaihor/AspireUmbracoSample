{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AppSettings": {
    "RenderingMode": "Mvc",
    "CacheSharedComponents": true
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "517b353d-dea5-46cb-b95d-d3436420770b",
        "UseHttps": true,
        "NoNodesViewPath": "~/umbraco/UmbracoWebsite/Deploy/NoNodes.cshtml",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing",
        "ModelsDirectory": "~/../AspireUmbracoSample.UmbracoModels",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsNamespace": "AspireUmbracoSample.UmbracoModels"
      }
    }
  },
  "uSync": {
    "Settings": {
      "UiEnabledGroups": "All",
      "ExportOnSave": "None",
      "IsRootSite": false,
      "LockRoot": false,
      "RootFolder": "./uSync/",
      "Folders": [
        "./uSync"
      ]
    }
  },
  "RulesBasedOutputCache": {
    "OutputCacheEnabled": true,
    "AdminPanel": {
      "AdminPanelEnabled": true,
      "AdminPanelApiEnabled": true,
      "AdminPanelUrl": "/umbraco/outputCache"
    },
    "SqlConnectionStringName": "umbracoDbDSN",
    "DefaultCacheTimeout": "00:30:00",
    "CachingRules": [
      {
        "RuleTemplate": "/*",
        "RuleAction": "Allow",
        "RuleType": "Regex",
        "VaryByQueryString": true,
        "VaryByData": true
      },
      {
        "RuleTemplate": "/install*",
        "RuleAction": "Disallow",
        "RuleType": "Regex"
      },
      {
        "RuleTemplate": "/umbraco*",
        "RuleAction": "Disallow",
        "RuleType": "Regex"
      },
      {
        "RuleTemplate": "/App_Plugins*",
        "RuleAction": "Disallow",
        "RuleType": "Regex"
      },
      {
        "RuleTemplate": "/umbidlocallogin*",
        "RuleAction": "Disallow",
        "RuleType": "Regex"
      }
    ]
  }
}
